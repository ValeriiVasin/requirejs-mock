{
    "parserOptions": {
        "sourceType": "script"
    },
    "env": {
        "amd": true,
        "node": true,
        "jasmine": true,
    },
    "globals": {},
    "rules": {
        "no-shadow": 0,
        "no-new": 0,
        "no-underscore-dangle": 0,
        "no-multi-spaces": 0,
        "consistent-return": 0,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "no-multiple-empty-lines": 2,
        "quotes": [
            2,
            "single"
        ],
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "wrap-iife": [
            2,
            "inside"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": false
            }
        ],
        "no-unused-vars": 2,
        "strict": [
            2,
            "global"
        ],
        "comma-style": [
            2,
            "last"
        ],
        "keyword-spacing": [
            "error",
            {
                "after": true
            }
        ],
        "space-before-function-paren": [
            "error",
            "always"
        ],
        "space-in-parens": 2,
        "space-before-blocks": 2
    }
}